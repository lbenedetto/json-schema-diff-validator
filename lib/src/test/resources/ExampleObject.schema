{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "$defs" : {
    "SomePojo-nullable" : {
      "anyOf" : [ {
        "type" : "null"
      }, {
        "$ref" : "#/$defs/SomePojo"
      } ]
    },
    "SomePojo" : {
      "type" : "object",
      "properties" : {
        "someField" : {
          "type" : "string"
        }
      },
      "required" : [ "someField" ]
    }
  },
  "type" : "object",
  "properties" : {
    "fieldWhichReferencesNonNullTypeDef" : {
      "$ref" : "#/$defs/SomePojo"
    },
    "fieldWhichReferencesNullableTypeDef" : {
      "$ref" : "#/$defs/SomePojo-nullable"
    },
    "nullableFieldWithAnyOf" : {
      "anyOf" : [ {
        "type" : "null"
      }, {
        "type" : "integer"
      } ]
    },
    "fieldWithAnyOf" : {
      "anyOf" : [ {
        "type" : "boolean"
      }, {
        "type" : "integer"
      } ]
    },
    "listOfEnumValues" : {
      "type" : "array",
      "items" : {
        "type" : "string",
        "enum" : [ "VALUE_A", "VALUE_B" ]
      }
    },
    "listOfObjects" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "someRequiredField" : {
            "type" : "string",
            "format" : "date-time"
          },
          "someOptionalField" : {
            "type" : "string"
          }
        },
        "required" : [ "someRequiredField" ]
      }
    },
    "someEnumValue" : {
      "type" : "string",
      "enum" : [ "VALUE_A", "VALUE_B" ]
    },
    "someIntegerField": {
      "type" : "integer"
    },
    "someNullableField" : {
      "type" : [ "integer", "null" ]
    }
  },
  "required" : [ "someIntegerField", "listOfEnumValues", "listOfObjects", "someEnumValue" ]
}
